var file_element = $('#file-<%= @file.id %>');

$('#mainModal').modal('hide');
file_element.html('<%= j render 'file_string', file: @file %>').effect('highlight', { color: 'yellow' }, 1500);

<% if @file.status %>
  file_element.addClass('text-line-through');
<% else %>
  file_element.removeClass('text-line-through');
<% end %>